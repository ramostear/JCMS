#brand {
    width: 50px;
    height: 50px;
}
.navbar-brand {
    padding: 15px 15px;
}
#center-content{
    width: 100%;
    margin-top:-20px;
}
.dropdown-menu-big {
    ul {
        margin-top: 3px;
        margin-left: -10px;
        ul {
            .header {
                margin-left: 3px;
                font-weight: 600;
                color: @font-forbidden;
            }
            li {
                height: 28px;
                a {
                    color: @color-gray-dark;
                    text-decoration: none;
                    display: block;
                    padding: 4px 0px 0px 10px;
                    height: 100%; 
                }
                a:visited,
                a:hover {
                    background-color: @link-block-hover;
                }
            }
        }
    }
}

.navbar {
    border-radius: 0px;
}

