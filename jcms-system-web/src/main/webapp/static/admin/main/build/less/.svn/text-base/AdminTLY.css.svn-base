.nav-basic {
  margin-bottom: 0;
  padding-left: 0;
  list-style: none;
}
.nav-basic:before,
.nav-basic:after {
  content: " ";/* 1 */
  display: table;/* 2 */
}
.nav-basic:after {
  clear: both;
}
.nav-basic > .nav-heading {
  padding: 8px 15px;
  color: #808080;
  font-weight: bold;
}
.nav-basic > li {
  position: relative;
  display: block;
}
.nav-basic > li > a {
  position: relative;
  display: block;
  padding: 8px 15px;
  -webkit-transition: all 0.2s cubic-bezier(0.175, .885, .32, 1);
  transition: all 0.2s cubic-bezier(0.175, .885, .32, 1);
  color: #222;
}
.nav-basic > li > a:hover,
.nav-basic > li > a:focus {
  text-decoration: none;
  background-color: #e8edf0;
  color: #222222;
}
.nav-basic > li.disabled > a {
  color: #808080;
}
.nav-basic > li.disabled > a:hover,
.nav-basic > li.disabled > a:focus {
  color: #808080;
  text-decoration: none;
  background-color: transparent;
  cursor: not-allowed;
}
.nav-basic > li > a > img {
  max-width: none;
}
.nav-block > li {
  float: left;
}
.nav-block > li > a {
  color: #333333;
  background-color: #d9e1e5;
  border-radius: 0px;
}
.nav-block > li + li {
  margin-left: 8px;
}
.nav-block > li.active > a,
.nav-block > li.active > a:hover,
.nav-block > li.active > a:focus {
  color: #f1f1f1;
  background-color: #587381;
}
.nav-block > li.active > a .caret,
.nav-block > li.active > a:hover .caret,
.nav-block > li.active > a:focus .caret {
  border-top-color: #1A2226;
  border-bottom-color: #1A2226;
}
