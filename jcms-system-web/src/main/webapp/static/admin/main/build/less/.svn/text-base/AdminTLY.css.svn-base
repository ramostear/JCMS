/*
 * sidebar-sm
 * 缩小状态下的导航栏样式
 * ======================
 */
.sidebar-sm {
  width: 50px;
}
.sidebar-sm .side-menu {
  user-select: none;
  -moz-user-select: none;
  -webkit-user-select: none;
  -ms-user-select: none;
  -khtml-user-select: none;
  width: 100%;
  max-width: 200px;
  padding: 0px 0px 0px 0px;
}
.sidebar-sm .side-menu ul {
  padding: 0px 0px 0px 0px;
}
.sidebar-sm .side-menu .ul-header {
  margin-left: 30px;
  min-width: 160px;
  display: none;
}
.sidebar-sm .side-menu .link-none,
.sidebar-sm .side-menu .link {
  z-index: 1000;
  color: #f1f1f1;
  border-left: 3px solid transparent;
  cursor: pointer;
  display: block;
  padding: 10px 15px 12px 35px;
  color: #cad5db;
  font-size: 14px;
  font-weight: 500;
  position: relative;
  -webkit-transition: all 0s ease;
  -o-transition: all 0s ease;
  transition: all 0s ease;
  width: 50px;
  height: 40px;
}
.sidebar-sm .side-menu .link:hover {
  z-index: 1000;
  width: 230px;
  color: #f1f1f1;
  background-color: #1c2529;
  border-left: 3px solid #0288d1;
  -webkit-transition: all 0s ease;
  -o-transition: all 0s ease;
  transition: all 0s ease;
}
.sidebar-sm .side-menu .submenu {
  z-index: 1000;
  display: none;
  font-size: 14px;
  margin-left: 30px;
  position: absolute;
}
.sidebar-sm .side-menu .submenu a {
  z-index: 1000;
  display: block;
  text-decoration: none;
  color: #acbdc6;
  background-color: #2c3b41;
  padding: 12px;
  margin-left: 20px;
  width: 180px;
}
.sidebar-sm .side-menu .submenu a:hover {
  color: #f1f1f1;
}
.sidebar-sm .side-menu .submenu li a span {
  margin-left: 10px;
  display: block;
  width: 160px;
}
.sidebar-sm .side-menu li {
  max-height: 40px;
}
.sidebar-sm .side-menu li i {
  position: absolute;
  top: 13px;
  left: 12px;
  font-size: 14px;
  color: #cad5db;
}
.sidebar-sm .side-menu li i.icon-chevron-left {
  display: none;
}
.sidebar-sm .side-menu li.open {
  max-height: 40px;
}
.sidebar-sm .side-menu li.open > a {
  width: 230px;
  color: #f1f1f1;
  background-color: #1c2529;
  border-left: 3px solid #0288d1;
  -webkit-transition: all 0s ease;
  -o-transition: all 0s ease;
  transition: all 0s ease;
}
.sidebar-sm .side-user {
  user-select: none;
  -moz-user-select: none;
  -webkit-user-select: none;
  -ms-user-select: none;
  -khtml-user-select: none;
  background: #181f23;
  padding: 10px 10px 10px 9px;
}
.sidebar-sm .side-user .user-name {
  margin-left: 10px;
  font-size: 14px;
  color: #f1f1f1;
}
.sidebar-sm .side-user .user-name a {
  margin-left: 30px;
}
.sidebar-sm #head {
  width: 30px;
  height: 30px;
}
.sidebar-sm .search-group {
  margin-top: 20px;
}
.sidebar-sm .search-group input {
  width: 140px;
  height: 30px;
  background-color: #2C3B41;
  border: 0px;
  outline: none;
  border-top-left-radius: 3px;
  border-bottom-left-radius: 3px;
}
.sidebar-sm .search-group input[type="search"]:focus {
  background-color: #FFFFFF;
  border: none;
  outline: none;
}
.sidebar-sm .search-group button {
  background-color: #2C3B41;
  border: none;
  height: 30px;
  width: 40px;
  padding: 0px 0px 0px 0px;
  outline: none;
  border-top-right-radius: 3px;
  border-bottom-right-radius: 3px;
}
.sidebar-sm .search-group button[type="submit"]:focus {
  text-shadow: none;
  background-color: #FFFFFF;
  border: none;
  outline: none;
}
.sidebar-sm .side-btn {
  display: block;
  padding: 12px 0px 12px 0px;
  text-align: center;
  -webkit-transition: all 0s ease;
  -o-transition: all 0s ease;
  transition: all 0s ease;
}
.sidebar-sm .side-btn i {
  font-size: 18px;
  color: #cad5db;
}
.sidebar-sm .side-btn:hover {
  color: #f1f1f1;
  background-color: #1c2529;
  -webkit-transition: all 0s ease;
  -o-transition: all 0s ease;
  transition: all 0s ease;
}
